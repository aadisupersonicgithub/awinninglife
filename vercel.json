{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/.next"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/.next",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
